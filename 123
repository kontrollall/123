local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("Credits: TGMANKASKE")
local Section = Window:NewSection("Teleports")

-- Lista de mapas e posições
local mapPositions = {
    ["Fried Potato"] = Vector3.new(-43, 1023, 85),
    ["Cheesy Hill"] = Vector3.new(100, 950, 75),
    ["Spicy Mountain"] = Vector3.new(-150, 900, 60)
    -- Adicione mais posições aqui no formato: ["Nome"] = Vector3.new(x, y, z),
}

-- Pega os nomes dos mapas para o dropdown
local mapNames = {}
for name, _ in pairs(mapPositions) do
    table.insert(mapNames, name)
end

-- Cria o dropdown com os nomes dos mapas
Section:CreateDropdown("Selecione o Mapa", mapNames, function(selectedMap)
    local position = mapPositions[selectedMap]
    if position then
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            player.Character.HumanoidRootPart.CFrame = CFrame.new(position)
        end
    end
end)

-- Botão opcional para mostrar posições salvas (debug)
Section:CreateButton("Mostrar posições salvas", function()
    for name, pos in pairs(mapPositions) do
        print(name .. " = " .. tostring(pos))
    end
end)
